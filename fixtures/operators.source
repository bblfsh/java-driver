class Code {
	void code() {
		int i;
		i = 1 + 2;
		i = 1 - 2;
		i = 1 * 2;
		i = 1 / 2;
		i = 1 % 2;

		i++;
		i--;

		++i;
		--i;

		i = +i;
		i = -1;

		i = ~i;

		i = 1&2;
		i = 1^2;
		i = 1|2;
		i = 1 << 2;
		i = 1 >> 2;
		i = 1 >>> 2;

		i = (true)? 1 : 2;

		i += 1;
		i -= 1;
		i *= 1;
		i /= 1;
		i %= 1;
		i &= 1;
		i ^= 1;
		i |= 1;
		i <<= 1;
		i >>= 1;
		i >>>= 1;

		boolean b;

		b = !true;

		b = 1 == 2;
		b = 1 > 2;
		b = 1 >= 2;
		b = 1 < 2;
		b = 1 <= 2;

		b = new Boolean(true) instanceof Boolean;
	}
}
